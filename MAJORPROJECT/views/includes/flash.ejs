<% if( success && success.length>0)  { %>
<div class= "alert alert-success aert-dismissible fade show col-6 offset-3" role = "alert">
    <%= success %>
    <button type="button" class="btn-close" data-bs-dismiss="alert" aria-label="Close">

    </button>
</div>
<% } %>

<% if( error && error.length>0)  { %>
    <div class= "alert alert-danger aert-dismissible fade show col-6 offset-3" role = "alert">
        <%= error %>
        <button type="button" class="btn-close" data-bs-dismiss="alert" aria-label="Close">
            
        </button>
    </div>
    <% } %>